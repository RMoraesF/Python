num1=int(input('Digite um número inteiro: '))
num2=int(input('Digite mais um número inteiro: '))
num3=int(input('Digite o último número inteiro: '))
if num1 < num2 and num1 < num3:
    print('O menor número inteiro digitado foi: ',num1)
else:
        if num2 < num1 and num2 < num3:
            print ('O menor número inteiro digitado foi: ',num2)
        else:
                if num3 < num2 and num3 < num1:
                    print('O menor número inteirod digitado foi: ',num3)
